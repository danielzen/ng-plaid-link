{
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "name": "ng-plaid-link",
  "description": "Angular Plaid Link Directive",
  "version": "2.0.2",
  "authors": [
    {
      "name": "Daniel Zen",
      "url": "https://zen.digital"
    },
    {
      "name": "Mike Roberts",
      "url": "https://mikeroberts.engineer"
    }
  ],
  "scripts": {
    "build": "ng-packagr -p package.json",
    "pack-dist": "npm pack ./dist/ng-plaid-link",
