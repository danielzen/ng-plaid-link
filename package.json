  "repository": {
    "type": "git",
    "url": "https://github.com/danielzen/ng-plaid-link"
  },
  "peerDependencies": {
    "@angular/common": "^13.0.0 || ^12.0.0 || ^11.0.0 ||  ^10.0.0 || ^9.0.0 || ^8.0.0 || ^7.0.0 || ^6.0.0",
    "@angular/core": "^13.0.0 || ^12.0.0 || ^11.0.0 ||  ^10.0.0 || ^9.0.0 || ^8.0.0 || ^7.0.0 || ^6.0.0"
  },
  "devDependencies": {
    "@angular/common": "^10.2.5",
    "@angular/compiler": "^10.2.5",
    "@angular/compiler-cli": "^10.2.5",
    "@angular/core": "^10.2.5",
    "@types/jasmine": "^3.10.2",
    "ng-packagr": "^10.1.1",
    "typescript": "~3.9.4"
  }
}
